{% extends 'layout/layout.html.twig' %}
{% block body %}  
    {% block about %}

    {% endblock %}
    {% block services %}
        <!--==========================
          Services Section
        ============================-->
        <section id="services">
            <div class="container  ">
                <div class="row">
                    <div class="col-md-12">
                      
                        <h3 class="section-title">  {{'services.title'|trans}}</h3>
                        <div class="section-title-divider"></div>
                        <p class="section-description">{{'services.intro'|trans}}</p>
                    </div>
                </div>
                

                <div class="row">
                    <div class="col-md-4 service-item">
                        <div class="service-icon"><i class="glyphicon glyphicon-book"></i></div>
                        <h4 class="service-title"><a href="">{{'services.service1'|trans}} </a></h4>
                        <p class="service-description"></p>
                    </div>
                    <div class="col-md-4 service-item">
                        <div class="service-icon"><i class="glyphicon glyphicon-facetime-video"></i></div>
                        <h4 class="service-title"><a href=""> {{'services.service2'|trans}}</a></h4>
                        <p class="service-description"></p>
                    </div>
                    <div class="col-md-4 service-item">
                        <div class="service-icon"><i class="glyphicon glyphicon-eye-open"></i></div>
                        <h4 class="service-title"><a href="">{{'services.service3'|trans}}</a></h4>
                        <p class="service-description"></p>
                    </div>
                   

                </div>
                <div class="row">
                    <div class="col-md-6 service-item">
                        <div class="service-icon"><i class="glyphicon glyphicon-play"></i></div>
                        <h4 class="service-title"><a href="{{ path('gallery') }}">{{'services.service5'|trans}} </a></h4>
                        <p class="service-description"></p>
                    </div>
                    <div class="col-md-6 service-item">
                        <div class="service-icon"><i class="glyphicon glyphicon-facetime-video"></i></div>
                        <h4 class="service-title"><a href=""> {{'services.service4'|trans}}</a></h4>
                        <p class="service-description"></p>
                    </div>
                    

                </div>
            </div>  
        </section>
    {% endblock %}
    {% block slider %}

    {% endblock %}

    {% block testimonials %}

    {% endblock %}

    {% block team %}
        <!--==========================
          Team Section
        ============================-->    
        <section id="team">
            <div class="container  ">
                <div class="row">
                    <div class="col-md-12">
                        <h3 class="section-title">{{'equipe.title'|trans}}</h3>
                        <div class="section-title-divider"></div>
                      
                        </p>
                    </div>
                </div>

                <div class="row">

                    {% for journalist in journalists %}

                        
                           
                            <div class="col-md-3">
                                <div class="member">
                                     <a href="{{ path('homepage') }}">
                                    <div class="pic"><img style="width: 90%; height: auto;" src="{{asset(journalist.token|slice(1))}}" alt=""  class="img-circle"></div>
                                     <a/>
                                    <h4 class="capitale">{{ journalist.username }}  </h4>

                                   
                                 </div>
                                
                            </div>
                       
                    {% endfor %}


                </div>  
            </div>
        </section>
    {% endblock %}

    {% block subscrbe %}
        {{parent()}}
    {% endblock %}
    {% block contact %}
        {{parent()}}
    {% endblock %}

{% endblock %}   


